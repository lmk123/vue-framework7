{
  "name": "vue-framework7",
  "version": "0.5.7",
  "description": "Framework7 components built with Vue.js",
  "main": "dist/vue-framework7.nosh.common.js",
  "module": "dist/vue-framework7.esm.js",
  "unpkg": "dist/vue-framework7.js",
  "keywords": [
    "vue",
    "framework7",
    "component"
  ],
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmk123/vue-framework7.git"
  },
  "scripts": {
    "start": "cd demo-site && yarn start",
    "deploy:demo": "cd demo-site && yarn run deploy",
    "lint": "eslint --ignore-pattern '/dist/' --ignore-pattern '/demo-site/' \"**/*.{js,vue}\"",
    "build": "node build/build.js",
    "prepublishOnly": "yarn run build"
  },
  "peerDependencies": {
    "framework7": "^1.4.2",
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^4.0.0",
    "rollup": "^0.45.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-vue": "^2.3.1",
    "uglify-js": "^3.0.4"
  },
  "author": "Milk Lee (https://github.com/lmk123)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lmk123/vue-framework7/issues"
  },
  "homepage": "https://github.com/lmk123/vue-framework7#readme"
}
